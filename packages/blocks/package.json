{
  "name": "@orion-ds/blocks",
  "version": "1.0.0",
  "description": "Premium sections and templates for Orion Design System - pre-built UI blocks for marketing, app, and SaaS interfaces",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": ["**/*.css"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.cjs",
      "types": "./dist/client.d.ts"
    },
    "./sections": {
      "import": "./dist/sections/index.mjs",
      "require": "./dist/sections/index.cjs",
      "types": "./dist/sections/index.d.ts"
    },
    "./sections/*": {
      "import": "./dist/sections/*/index.mjs",
      "require": "./dist/sections/*/index.cjs",
      "types": "./dist/sections/*/index.d.ts"
    },
    "./templates": {
      "import": "./dist/templates/index.mjs",
      "require": "./dist/templates/index.cjs",
      "types": "./dist/templates/index.d.ts"
    },
    "./templates/*": {
      "import": "./dist/templates/*/index.mjs",
      "require": "./dist/templates/*/index.cjs",
      "types": "./dist/templates/*/index.d.ts"
    },
    "./styles.css": "./dist/blocks.css"
  },
  "files": ["dist"],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "@orion-ds/react": "workspace:*",
    "lucide-react": ">=0.400.0",
    "recharts": ">=3.0.0",
    "@dnd-kit/core": ">=6.0.0",
    "@dnd-kit/sortable": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "recharts": {
      "optional": true
    },
    "@dnd-kit/core": {
      "optional": true
    },
    "@dnd-kit/sortable": {
      "optional": true
    }
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "type-check": "tsc --noEmit",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "keywords": [
    "orion",
    "design-system",
    "blocks",
    "sections",
    "templates",
    "react",
    "ui",
    "components"
  ],
  "author": "Orion Design System",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0 || ^19.0.0",
    "@types/react-dom": "^18.2.0 || ^19.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@orion-ds/react": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "jsdom": "^23.0.0",
    "@vitest/ui": "^1.0.0"
  }
}
